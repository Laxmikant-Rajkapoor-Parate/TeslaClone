{
  "expo": {
    "name": "Tesla Clone",
    "slug": "tesla-clone",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    },
    "description": "This apps shows the frontend view of the tesla.com website. This app has the same scrolling effect found on the TikTok app and the tesla website.\nCurrently, the functionalities aren't added. It will be added in the future.",
    "githubUrl": "https://github.com/Laxmikant-Rajkapoor-Parate/TeslaClone.git"
  }
}
